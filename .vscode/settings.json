{
  "files.associations": {
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".eslintignore": "ignore"
  },

  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "typescriptreact"
  ],

  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,

  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    ".erb/dll": true,
    "release/{build,app/dist}": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "package-lock.json": true,
    "*.{css,sass,scss}.d.ts": true
  },
  "java.project.referencedLibraries": [
    "lib/**/*.jar",
    "src\\native\\aspose-pdf-22.6.0.jar",
    "src\\native\\aspose-words-15.8.0-jdk16.jar",
    "native\\aspose-pdf-22.6.0.jar",
    "native\\aspose-words-15.8.0-jdk16.jar"
  ]
}
